{
  "name": "indicab",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest --config config/vitest.config.ts src/tests/components",
    "test:api": "vitest --config config/vitest.config.ts src/tests/api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test --config config/playwright.config.ts src/tests/e2e/visual-regression.spec.ts",
    "test:accessibility": "playwright test --config config/playwright.config.ts src/tests/e2e/accessibility.spec.ts",
    "test:performance": "playwright test --config config/playwright.config.ts src/tests/e2e/performance.spec.ts",
    "test:load": "k6 run src/tests/load/load-test.js",
    "test:load:local": "k6 run --env BASE_URL=http://localhost:3000 src/tests/load/load-test.js",
    "test:load:staging": "k6 run --env BASE_URL=https://staging.indicab.com src/tests/load/load-test.js",
    "test:load:prod": "k6 run --env BASE_URL=https://indicab.com src/tests/load/load-test.js",
    "test:load:report": "k6 run --out json=reports/load-test-results.json src/tests/load/load-test.js",
    "test:all": "npm run test:unit && npm run test:api && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:ci": "playwright test --config config/playwright.config.ts --reporter=dot",
    "lighthouse": "lhci autorun --config config/lighthouserc.js",
    "test:db": "ts-node scripts/database/db-utils.ts test",
    "test:db:validate": "ts-node scripts/database/db-utils.ts validate",
    "test:api:user": "ts-node scripts/api/api-utils.ts user",
    "test:api:vehicle": "ts-node scripts/api/api-utils.ts vehicle",
    "test:api:booking": "ts-node scripts/api/api-utils.ts booking",
    "fix:code": "ts-node scripts/utils/code-quality.ts",
    "fix:types": "ts-node scripts/utils/code-quality.ts --fix-types",
    "fix:lint": "ts-node scripts/utils/code-quality.ts --fix-lint",
    "fix:document": "ts-node scripts/utils/code-quality.ts --document-errors",
    "analyze": "ts-node -P tsconfig.node.json scripts/utils/analyze.ts",
    "analyze:coverage": "ts-node -P tsconfig.node.json scripts/utils/analyze.ts --coverage",
    "analyze:legacy": "ts-node -P tsconfig.node.json scripts/utils/analyze.ts --legacy",
    "analyze:bundle": "ts-node -P tsconfig.node.json scripts/utils/analyze.ts --bundle",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test:ui": "vitest --ui",
    "test:perf": "vitest run --testNamePattern=perf",
    "test:a11y": "vitest run --testNamePattern=a11y"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "@heroicons/react": "^2.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/instrumentation": "^0.201.1",
    "@opentelemetry/sdk-node": "^0.201.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/nextjs": "^9.20.0",
    "@stripe/react-stripe-js": "^3.5.1",
    "@stripe/stripe-js": "^6.1.0",
    "@upstash/ratelimit": "^1.2.1",
    "@upstash/redis": "^1.34.9",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.8",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bun": "^1.2.9",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.12.1",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.511.0",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "next": "^15.3.2",
    "next-auth": "^4.24.5",
    "next-i18next": "^15.4.2",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.242.2",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.1",
    "react-leaflet": "^4.2.1",
    "stripe": "^17.7.0",
    "tailwind-merge": "^3.0.2",
    "typescript": "^5.3.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.0",
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/eslintrc": "^3.0.2",
    "@hookform/resolvers": "^5.0.1",
    "@next/bundle-analyzer": "^15.2.4",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^7.6.17",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.18",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.0.11",
    "jest-axe": "^7.0.0",
    "jsdom": "^26.0.0",
    "k6": "^0.0.0",
    "lint-staged": "^15.2.2",
    "playwright": "^1.40.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.3.0",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vitest-mock-extended": "^3.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@sentry/cli"
  ]
}
